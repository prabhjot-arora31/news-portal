<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/home.css">
</head>
<body style="position: relative;">
    <div class="top">
        <div class="imgContainer">
            <img src="data:image/png;base64," alt="">
        </div>
        <h2 style="text-align: center;">News Portal</h2>
    </div>
  <a href="/logout" style="background-color: black; color: white; border: none; text-decoration: none; padding: 0.5rem;">Logout</a>
  <br>
  <br><br>
  <% if(email != "admin@g.co"){ %>
    <div class="big-main">
        <% if(showTabs){ %>
        <div class="tabs">
            <h3 onclick="getData('/home/political' , this.id)" id="p">Political</h3>
            <h3 onclick="getData('/home/finance' , this.id)" id="f">Finance</h3>
            <h3 onclick="getData('/home/sports' , this.id)" id="s">Sports</h3>
            <h3 onclick="getData('/home/entertainment' , this.id)" id="e">Entertainment</h3>
        </div>
        <% }%>
        <div style="padding: 0.7rem;">
            <% if(!showTabs){ %>
                <a href="/home" style="background-color: black; color: white; padding:0.17rem 0.65rem; text-decoration: none;">Main Menu</a>
<% }%>
<% if(date != ""){ %>
    <br>
<h3 style="text-align: center;">
   Showing results for: <%= date %>
</h3>
<% } %>
            <h5 style="text-align: center;">
                <% if(showTabs){ %>
                    Or 
                    <% } %>
                    <br>
                    Select date
                <form action="/home/news-by-date" method="post">
                    <input type="date" id="" name="date" style="padding: 0.25rem;">
                    <button style="background-color: black; color: white; border: none; cursor: pointer; padding: 0.35rem;">Filter</button>
                </form>
            </h5>
        </div>
        <div class="main" id="main">
            <%
            if(data.length <= 0){%>
                <h3 style="margin: 20px auto;">No Data Found</h3>
            <%}else{
            for(var i = 0 ; i < data.length ; i++){ %>
                <div style="padding: 0.7rem; background-color: lightgray; cursor: pointer;" class="box" id="box" onclick="clicking(`<%= data[i].news %>`)">
                   <h3 style="text-align: center;" id="heading">
                       <%= data[i].title %>
                   </h3>
                   <h6 style="background-color: black; color: white; padding: 0.32rem; border-radius: 0.6rem;"><%= data[i].category %></h6>
                   <div style="width: 400px; margin: 0 auto;">
                       <img style="width: 100%;" src=' <%= data[i].imageUrl %>' alt="" id="img">
                   </div>
                   <p style="font-size: 11.2px;" id="time">Updated: <%= data[i].createdAt %></p>
                </div>
            <% }} %>
        </div>
    </div>  
<% }else{ %>
       <div style="display: flex; flex-wrap: wrap; gap: 20px; justify-content: center;">
        <div class="main2">
            <h3 style="text-align: center;">Admin Dashboard</h3>
            <h4 style="text-align: center;">Add News</h4>
            <form class="inner" method="post" action="/post-news">
                <input type="text" placeholder="Enter Title" name="title">
                <input type="text" placeholder="Enter Image URL" name="imageUrl">
                <textarea name="news" placeholder="Enter News" id="" cols="30" rows="10"></textarea>
                <p style="font-size: 14px;margin: 0;">Select Category</p>
                <select name="category" id="category">
                    <option value="political">Political</option>
                    <option value="finance">Finance</option>
                    <option value="sports">Sports</option>
<option value="entertainment">Entertainment</option>
                </select>
                <button style="background-color: black; color: white; padding: 0.54rem 1.2rem; border: none;cursor: pointer;">Add</button>
            </form>
          </div>
         
          <div class="big-main" style="margin: 0;">
            <% if(showTabs){ %>
            <div class="tabs">
                <h3 onclick="getData('/home/political' , this.id)" id="p">Political</h3>
                <h3 onclick="getData('/home/finance' , this.id)" id="f">Finance</h3>
                <h3 onclick="getData('/home/sports' , this.id)" id="s">Sports</h3>
                <h3 onclick="getData('/home/entertainment' , this.id)" id="e">Entertainment</h3>
            </div>
            <% }%>
            <div style="padding: 0.7rem;">
                <% if(!showTabs){ %>
                    <a href="/home" style="background-color: black; color: white; padding:0.17rem 0.65rem; text-decoration: none;">Main Menu</a>
    <% }%>
    <% if(date != ""){ %>
        <br>
    <h3 style="text-align: center;">
        Showing results for: <%= date %>

    </h3>
    <% } %>
                <h5 style="text-align: center;">
                    <% if(showTabs){ %>
                        Or 
                        <% } %>
                        <br>
                        Select date
                    <form action="/home/news-by-date" method="post">
                        <input type="date" id="" name="date" style="padding: 0.25rem;">
                        <button style="background-color: black; color: white; border: none; cursor: pointer; padding: 0.35rem;">Filter</button>
                    </form>
                </h5>
            </div>
            <div class="main" id="main">
                <%
                if(data.length <= 0){%>
                    <h3 style="margin: 20px auto;">No Data Found</h3>
                <%}else{
                for(var i = 0 ; i < data.length ; i++){ %>
                    <div style="padding: 0.7rem; background-color: lightgray; cursor: pointer;" class="box" id="box" onclick="clicking(`<%= data[i].news %>`)">
                       <h3 style="text-align: center;" id="heading">
                           <%= data[i].title %>
                       </h3>
                   <h6 style="background-color: black; color: white; padding: 0.32rem; border-radius: 0.6rem;"><%= data[i].category %></h6>
                       <div style="width: 400px; margin: 0 auto;">
                           <img style="width: 100%;" src=' <%= data[i].imageUrl %>' alt="" id="img">
                       </div>
                       <p style="font-size: 11.2px;" id="time">Updated: <%= data[i].createdAt %></p>
                    </div>
                <% }} %>
            </div>
        </div> 
       </div> 
        <% } %>
<script>
    const box = document.getElementsByClassName('box');
    document.getElementById('p').style.backgroundColor = 'lightgray'
    var main = document.getElementById("main"); 
   function clicking(news){
            const big_main = document.getElementsByClassName('big-main');
        big_main[0].style.opacity = '0.6'
        big_main[0].style.opacity = '0.6'
        const newDiv = document.createElement('div');
        const cross = document.createElement('div');
        cross.innerHTML = 'X'
        cross.style.position = 'absolute';
        cross.style.top = '4.3px';
        cross.style.right = '4.3px'
        cross.style.cursor = 'pointer';
        cross.addEventListener('click', function(){
        big_main[0].style.opacity = '1'
        newDiv.style.display = 'none'
        });
        newDiv.style.position = 'absolute';
        newDiv.style.top = '20px'
        newDiv.style.left = '0';
        newDiv.style.right = '0';
        newDiv.style.margin = '0 auto'
        newDiv.style.padding = '0.7rem';
        newDiv.style.width = '320px'
        newDiv.style.backgroundColor = 'black';
        newDiv.style.color = 'white';
        newDiv.style.overflowY = 'auto';
        newDiv.style.display = 'flex';
        newDiv.style.flexDirection = 'column';
        // newDiv.style.justifyContent = 'center';
        // newDiv.style.alignItems = 'center';
            newDiv.style.height = '400px'
        const p = document.createElement('h4');
        p.innerHTML = `${news}`;
        // p.style.marginTop = '60px'
        newDiv.appendChild(cross);
        newDiv.appendChild(p);

            document.getElementsByTagName('body')[0].appendChild(newDiv);
        }
   async function getData(endpoint , id){
    if(id == 'p')
    {
        document.getElementById('p').style.backgroundColor = 'lightgray'
        document.getElementById('f').style.backgroundColor = 'white'
        document.getElementById('s').style.backgroundColor = 'white'
        document.getElementById('e').style.backgroundColor = 'white'

    }
    else if(id == 'f')
    {
        document.getElementById('f').style.backgroundColor = 'lightgray'
        document.getElementById('p').style.backgroundColor = 'white'
        document.getElementById('s').style.backgroundColor = 'white'
        document.getElementById('e').style.backgroundColor = 'white'

    }
    else if(id == 's')
    {
        document.getElementById('s').style.backgroundColor = 'lightgray'
        document.getElementById('p').style.backgroundColor = 'white'
        document.getElementById('f').style.backgroundColor = 'white'
        document.getElementById('e').style.backgroundColor = 'white'

    }
    else if(id == 'e')
    {
        document.getElementById('e').style.backgroundColor = 'lightgray'
        document.getElementById('s').style.backgroundColor = 'white'
        document.getElementById('p').style.backgroundColor = 'white'
        document.getElementById('f').style.backgroundColor = 'white'
    }
    // start
    
    console.log(this);
    main.innerHTML = ''
    fetch(endpoint).then(data => data.json()).then(data => {
        console.log(data)
        data.data.forEach(item => {
            const div = document.createElement("div");
    div.style.backgroundColor = 'lighgray';
    div.style.padding = '0.7rem'
    div.style.backgroundColor = 'lightgray'
    div.style.cursor = 'pointer';
    div.style.maxHeight = '410px'
    const h3 = document.createElement("h3");
    h3.style.textAlign = 'center';
    h3.innerHTML = `${item.title}`;
    const imgDiv = document.createElement("div");
    imgDiv.style.width = '400px'
    imgDiv.style.margin = '0 auto'
    const img = document.createElement("img");
    img.style.width = '100%'
    img.src = `${item.imageUrl}`
    const p = document.createElement("p");
    p.style.fontSize = '11.2px';
    p.id = 'time';
    p.innerHTML = `Updated: ${item.createdAt}`;
    imgDiv.appendChild(img);
    div.appendChild(h3);
    div.appendChild(imgDiv);
    div.appendChild(p);
    div.id = 'box';
    div.addEventListener('click', function(){
        const big_main = document.getElementsByClassName('big-main');
        big_main[0].style.opacity = '0.6'
        const newDiv = document.createElement('div');
        const cross = document.createElement('div');
        cross.innerHTML = 'X'
        cross.style.position = 'absolute';
        cross.style.top = '4.3px';
        cross.style.right = '4.3px'
        cross.style.cursor = 'pointer';
        cross.addEventListener('click', function(){
        big_main[0].style.opacity = '1'
        newDiv.style.display = 'none'
        });
        newDiv.style.position = 'absolute';
        newDiv.style.top = '20px'
        newDiv.style.left = '0';
        newDiv.style.right = '0';
        newDiv.style.margin = '0 auto'
        newDiv.style.padding = '0.7rem';
        newDiv.style.width = '320px'
        newDiv.style.backgroundColor = 'black';
        newDiv.style.color = 'white';
        newDiv.style.overflowY = 'auto';
        newDiv.style.display = 'flex';
        newDiv.style.flexDirection = 'column';
        // newDiv.style.justifyContent = 'center';
        // newDiv.style.alignItems = 'center';
            newDiv.style.height = '400px'
        const p = document.createElement('h4');
        p.innerHTML = `${item.news}`;
        // p.style.marginTop = '60px'
        newDiv.appendChild(cross);
        newDiv.appendChild(p);
        document.getElementsByTagName('body')[0].appendChild(newDiv);
    })
    main.appendChild(div)   
        });
        
    }).catch(error => console.log(error));
    }
    
</script>
</body>
</html>
